{
  "name": "http-ext",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "nx": "nx",
    "start": "run-p start:*",
    "start:app": "ng serve",
    "start:api": "json-server apps/sandbox/src/assets/bikes.json",
    "build": "ng build",
    "test": "ng test",
    "test:angular": "ng test angular --watch",
    "test:core": "ng test core --watch",
    "test:plugin-cache": "ng test plugin-cache --watch",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "limbo": "tools/limbo.sh",
    "merge-into-master": "tools/merge-into-master.sh",
    "publish:libs": "run-s _lerna:version \"affected:build --prod --all\" _publish:*",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "_publish": "yarn publish --non-interactive --access public",
    "_publish:angular": "yarn _publish dist/libs/angular",
    "_publish:core": "yarn _publish dist/libs/core",
    "_publish:plugin-cache": "yarn _publish dist/libs/plugin-cache",
    "_lerna:version": "lerna version --no-push --conventional-commits"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.2",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^9.0.0-rc.2",
    "@angular/compiler": "^9.0.0-rc.2",
    "@angular/core": "^9.0.0-rc.2",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^9.0.0-rc.2",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^9.0.0-rc.2",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.2",
    "@angular/router": "^9.0.0-rc.2",
    "@nrwl/angular": "^8.7.1",
    "core-js": "^3.0.0",
    "json-server": "^0.15.1",
    "lru-cache": "^5.1.1",
    "rxjs": "~6.5.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.2",
    "@angular-devkit/build-ng-packagr": "~0.900.0-rc.2",
    "@angular/cli": "9.0.0-rc.7",
    "@angular/compiler-cli": "^9.0.0-rc.2",
    "@angular/language-service": "^9.0.0-rc.2",
    "@nrwl/cypress": "8.9.0",
    "@nrwl/jest": "8.9.0",
    "@nrwl/workspace": "8.9.0",
    "@types/jest": "24.0.25",
    "@types/lru-cache": "^5.1.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.2.0",
    "cypress": "3.8.1",
    "dotenv": "8.2.0",
    "eslint": "6.8.0",
    "husky": "^3.0.9",
    "jest": "24.9.0",
    "jest-date-mock": "^1.0.7",
    "jest-preset-angular": "7.1.1",
    "lerna": "^3.17.0",
    "lint-staged": "9.5.0",
    "ng-packagr": "^9.0.0-rc.1",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "rxjs-marbles": "^5.0.3",
    "ts-jest": "24.2.0",
    "ts-node": "~8.5.2",
    "tslint": "~5.20.0",
    "typescript": "~3.6.0"
  },
  "workspaces": [
    "libs/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
