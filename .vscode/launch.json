  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "plugin-cache",
      "request": "launch",
      "args": [
        "--runInBand",
        "--config",
        "libs/plugin-cache/jest.config.js"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "disableOptimisticBPs": true,
      "program": "${workspaceFolder}/node_modules/jest/bin/jest"
    }
  ]
}