{
  "$schema": "https://raw.githubusercontent.com/renovatebot/renovate/master/renovate-schema.json",
  "extends": ["config:base"],
  "automerge": true,
  "automergeType": "branch",
  "baseBranches": [
    "develop"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "pin": {
    "automerge": false
  },
  "rangeStrategy": "update-lockfile"
}
